.container
  h2.buzz-title
    img(src='/img/jnj-kotesol-logo-blue.png')
    span Feedback
  ul.buzz-list
    li.buzz-list-item
      figure.attendee-img
        img(src='/img/people/#')
      .attendee-img-shadow
      .attendee-img-shadow-yellow
      p.attendee-info
        span.attendee-name #
        span.attendee-location
          a.attendee-link(href='#') #
      p.attendee-comment #
  // Begin Mailchimp Signup Form
  #mc_embed_signup
    form#mc-embedded-subscribe-form.subscriber-list.validate(action='https://jnjkotesol.us17.list-manage.com/subscribe/post?u=97baefed34e20adff64a02dfc&id=4080cfa5a5', method='post', name='mc-embedded-subscribe-form', target='_blank', novalidate='')
      // <div id="mc_embed_signup_scroll">
      h2.subscriber-list-title Subscribe to the JNJ KOTESOL email list
      #mce-responses.clear
        #mce-error-response.response(style='display:none')
        #mce-success-response.response(style='display:none')
      // real people should not fill this in and expect good things - do not remove this or risk form bot signups
      div(style='position: absolute; left: -5000px;', aria-hidden='true')
        input(type='text', name='b_97baefed34e20adff64a02dfc_4080cfa5a5', tabindex='-1', value='')
      input#mce-EMAIL.subscriber-email.form-control(type='email', placeholder='Your Email', value='', name='EMAIL')
      input#mc-embedded-subscribe.button.subscriber-submit(type='submit', value='Get on the list!', name='subscribe')
      // </div>
  // End mc_embed_signup
